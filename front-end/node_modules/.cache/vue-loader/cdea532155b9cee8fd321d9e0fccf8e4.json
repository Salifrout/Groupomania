       },\r\n                body: JSON.stringify(BODY),\r\n                redirect: 'follow'\r\n            };\r\n\r\n            fetch(\"http://localhost:3000/api/comment/\" + user_email, newPost)\r\n            .then(response => response.text())\r\n            .then(result => console.log( result ))\r\n            .then(() => location.reload())\r\n            .catch(error => console.log( error ));\r\n        },\r\n        deleteComment() {\r\n\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n#post {\r\n    &Forum {\r\n    margin: auto;\r\n    padding: 12px;\r\n    border: 4mm ridge red;\r\n    box-shadow: 8px 3px yellow, -1em 0 .4em plum;\r\n    width: 75%;\r\n    }\r\n    &Title {\r\n        margin-bottom: 20px;\r\n        border: 2px solid purple;   \r\n        background-color: plum;\r\n        color: #fff;\r\n        font-weight: 900;\r\n        text-align: center;\r\n        &Box {\r\n        border: 1mm solid peru;\r\n        background-color: maroon;\r\n        margin: 4px;\r\n        display: flex;\r\n        flex-wrap: wrap;\r\n        }\r\n        &_label {\r\n            font-weight: 900;\r\n        }\r\n    }\r\n    &Text {\r\n        margin-top: 15px;\r\n        margin-bottom: 5px;\r\n        &Block {\r\n            display: inline-block;\r\n            width: 100%;\r\n            text-align: justify;\r\n        }\r\n    }\r\n    &Author {\r\n        display: flex;\r\n        flex-wrap: wrap;\r\n        justify-content: space-between;\r\n    }\r\n    &Infos {\r\n        display: flex;\r\n        flex-wrap: wrap;\r\n        justify-content: space-between;\r\n        &Author {\r\n        display: flex;\r\n        flex-wrap: wrap;\r\n        font-weight: 700;\r\n        }\r\n        &Firstname, &Lastname, &Date {\r\n            font-weight: normal;\r\n        }\r\n    }\r\n    &Date {\r\n        font-weight: 700;\r\n    }\r\n}\r\n\r\n#chooseAct {\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    justify-content: space-be